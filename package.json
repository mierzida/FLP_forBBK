{
      "name": "responsive-soccer-lineup-display",
      "version": "1.7.0",
      "private": true,
      "dependencies": {
            "@radix-ui/react-collapsible": "^1.1.12",
            "@radix-ui/react-dialog": "^1.1.6",
            "@radix-ui/react-label": "^2.1.2",
            "@radix-ui/react-select": "^2.1.6",
            "class-variance-authority": "^0.7.1",
            "clsx": "*",
            "lucide-react": "^0.487.0",
            "react": "^18.3.1",
            "react-dom": "^18.3.1",
            "tailwind-merge": "*"
      },
      "devDependencies": {
            "@tailwindcss/postcss": "^4.1.14",
            "@types/node": "^20.10.0",
            "@types/react": "^19.2.2",
            "@types/react-dom": "^19.2.1",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "autoprefixer": "^10.4.21",
            "concurrently": "^8.2.0",
            "cross-env": "^7.0.3",
            "electron": "^26.0.0",
            "electron-builder": "^24.6.0",
            "postcss": "^8.5.6",
            "tailwindcss": "^4.1.14",
            "typescript": "^5.9.3",
            "vite": "6.3.5",
            "wait-on": "^7.0.1"
      },
      "scripts": {
            "dev": "vite --port 3000",
            "build": "vite build",
            "start": "vite preview",
            "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 NODE_ENV=development electron .\"",
            "electron:build": "npm run build && electron-builder"
      },
      "main": "electron/main.js",
      "build": {
            "appId": "com.example.soccer-lineup",
            "productName": "Soccer Lineup",
            "directories": {
                  "output": "dist"
            },
            "files": [
                  "build/**",
                  "node_modules/**",
                  "electron/**"
            ],
            "win": {
                  "target": [
                        "nsis"
                  ],
                  "icon": "build/icon.ico"
            }
      }
}
